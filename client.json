{
    "log": {
        "loglevel": "info"
    },
    "inbounds": [
        {
            "port": 8080,
            "protocol": "http",
            "sniffing": {
                "enabled": true,
                "destOverride": [
                    "http",
                    "tls"
                ]
            },
            "settings": {
                "accounts": [
                    {
                        "user": "USER",
                        "pass": "PASS"
                    }
                ],
                "allowTransparent": false,
                "userLevel": 0
            }
        }
    ],
    "outbounds": [
        {
            "tag": "proxy-vmess",
            "protocol": "vmess",
            "settings": {
                "vnext": [
                    {
                        "address": "SERVER_ADDR",
                        "port": SERVER_PORT,
                        "users": [
                            {
                                "id": "CLIENT_UUID",
                                "alterId": ALTER_ID
                            }
                        ]
                    }
                ]
            }
        },
        {
            "tag": "direct",
            "settings": {},
            "protocol": "freedom"
        }
    ]
}